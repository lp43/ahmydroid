<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:id="@+id/rl_set_notify"
  android:background="@drawable/background">

<ListView
android:id="@+id/list"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	
<TextView
	android:layout_width="wrap_content"
	android:layout_height="25dip"
	android:text="@string/location_setting"
	android:paddingBottom="1dip"
	android:paddingLeft="5dip"
	android:textSize="20sp"
	android:textColor="@color/black"/>

	
	<Button
	android:id="@+id/msg_btn"
	android:layout_width="140dip"
	android:layout_height="140dip"

	android:layout_marginTop="10dip"
	android:visibility="invisible"/>


	
	<Button
	android:id="@+id/fb_btn"
	android:layout_width="140dip"
	android:layout_height="140dip"

	android:text="facebook發文測試"
	android:layout_alignTop="@id/msg_btn"
	android:layout_toRightOf="@id/msg_btn"
	android:visibility="invisible"/>
		<Button
	android:id="@+id/twitter_btn"
	android:layout_width="140dip"
	android:layout_height="140dip"
	android:background="@drawable/twitter_noset"
	android:text="地點測試"
	android:layout_below="@id/fb_btn"
	android:visibility="invisible"/>
</ListView>
</FrameLayout>
